#!groovy
import uk.gov.hmcts.contino.Subscription

//noinspection GroovyUnusedAssignment
@Library("Infrastructure@feature/add-mgmt-sub-id-to-env") _

//noinspection GroovyAssignabilityCheck
properties([
  parameters([
    choice(name: 'ENVIRONMENT', choices: 'saat\nsprod\nsandbox', description: 'Environment where code should be build and deployed')
  ])
])

Subscription subscription = new Subscription(env)

def product = 'bulk-scan'

sharedInfrastructurePipeline(product, params.ENVIRONMENT, subscription.nonProdName)
